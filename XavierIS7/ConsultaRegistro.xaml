<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="XavierIS7.ConsultaRegistro">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Lista Estudiantes">
                
            </Label>
                 
              <ListView x:Name="ListarEstudiantes" ItemSelected="ListarEstudiantes_ItemSelected_1">
                  
                <ListView.ItemTemplate>
                    <DataTemplate>
                        <TextCell Text="{Binding Nombre}" Detail="{Binding Usuario}"/>
                    </DataTemplate>
                </ListView.ItemTemplate>
                        
              
              </ListView>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>